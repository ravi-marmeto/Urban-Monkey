  <style>
    .size-chart-label {cursor: pointer;font-size: 14px;margin: 0;text-decoration: underline;color: #0000ee;}
    .pop-content {display: none;}
    .scaleunit {display: inline-block;border: 2px solid #fcfcfc;margin-top: 0px;}
    .scaleunit-div h3 {color: #fff;font-size: 20px;margin: 5px auto;}
    .scaleunit button {color: #fff;font-weight: 600;background: #000000;font-size: 20px;padding: 5px;}
    button.size-active {background: #ffffff;color: #000;}
    .size-btn {outline:none;}
    .custom-model-main {
      text-align: center;overflow: hidden;position: fixed;top: 0;right: 0;bottom: 0;left: 0;display: none;
      -webkit-overflow-scrolling: touch;outline: 0;opacity: 0;
      -webkit-transition: opacity 0.15s linear, z-index 0.15;
      -o-transition: opacity 0.15s linear, z-index 0.15;
      transition: opacity 0.15s linear, z-index 0.15;
      z-index: -1;overflow-x: hidden;overflow-y: auto;
    }
    .custom-model-main.model-open {z-index: 99999;opacity: 1;overflow-x: hidden;padding: 0 20px;display: block;-webkit-box-align: center;-ms-flex-align: center;align-items: center;}
    .custom-model-inner {
      -webkit-transform: translate(0, -25%);
      -ms-transform: translate(0, -25%);
      transform: translate(0, -25%);
      -webkit-transition: -webkit-transform 0.3s ease-out;
      -o-transition: -o-transform 0.3s ease-out;
      transition: -webkit-transform 0.3s ease-out;
      -o-transition: transform 0.3s ease-out;
      transition: transform 0.3s ease-out;
      transition: transform 0.3s ease-out, -webkit-transform 0.3s ease-out;
      display: inline-block;
      vertical-align: middle;
      width: 750px;
      margin: 30px auto;
      max-width: 97%;background: #000000;
    }
    .model-open .custom-model-inner {
      -webkit-transform: translate(0, 0);
      -ms-transform: translate(0, 0);
      transform: translate(0, 0);
      position: relative;
      z-index: 999;
      top: 0px;
    }
    .model-open .bg-overlay {background: rgba(0, 0, 0, 0.6);z-index: 99;}
    .bg-overlay {background: rgba(0, 0, 0, 0);height: 100vh;width: 100%;position: fixed;left: 0;top: 0;right: 0;bottom: 0;z-index: 0;-webkit-transition: background 0.15s linear;-o-transition: background 0.15s linear;transition: background 0.15s linear;}
    .close-btn {position: absolute;right: -9px;top: -22px;cursor: pointer;z-index: 99;font-size: 30px;color: #fff;}
    .mobile-product-view{display:none;}
    .desktop-product-view{display:block;}
    @media screen and (max-width: 749px) {
      .mobile-product-view{display:block;}
      .desktop-product-view{display:none;}
    }
  </style>
